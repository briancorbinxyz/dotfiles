# Compilation flags
# export ARCHFLAGS="-arch x86_64"

# Set personal aliases, overriding those provided by oh-my-zsh libs,
# plugins, and themes. Aliases can be placed here, though oh-my-zsh
# users are encouraged to define aliases within the ZSH_CUSTOM folder.
# For a full list of active aliases, run `alias`.
#
# Example aliases
# alias zshconfig="mate ~/.zshrc"
# alias ohmyzsh="mate ~/.oh-my-zsh"

# pyenv
eval "$(pyenv init --path)" 
eval "$(pyenv init -)"
export PATH="/opt/homebrew/opt/qt@5/bin:$PATH"
export PATH="/opt/homebrew/opt/pyqt@5/bin:$PATH"

export ANDROID_SDK_ROOT=/Users/briancorbin/Library/Android/sdk
export PATH="/Users/briancorbin/Library/Android/sdk/platform-tools:$PATH"
export PATH="/Users/briancorbin/.local/bin:$PATH"
export PATH="$HOME/node_modules/.bin:$PATH"
export PATH="$HOME/npm-packages/bin:$PATH"
fpath+=~/.zfunc
autoload -Uz compinit && compinit
alias tf=terraform
alias ls=lsd
alias vi=nvim
alias ugh=fuck
eval $(thefuck --alias)


eval "$(zoxide init zsh)"

export FZF_COMPLETION_TRIGGER='``'

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
alias fzf="fzf --preview 'bat --style=numbers --color=always --line-range :500 {}'"
export MODULAR_HOME="/Users/briancorbin/.modular"
export PATH="/bin:$PATH"
export MODULAR_HOME="/Users/briancorbin/.modular"
export PATH="/Users/briancorbin/.modular/pkg/packages.modular.com_mojo/bin:$PATH"

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/opt/homebrew/Caskroom/miniconda/base/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/opt/homebrew/Caskroom/miniconda/base/etc/profile.d/conda.sh" ]; then
        . "/opt/homebrew/Caskroom/miniconda/base/etc/profile.d/conda.sh"
    else
        export PATH="/opt/homebrew/Caskroom/miniconda/base/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

# IntelliJ IDEA
export PATH="/Applications/IntelliJ IDEA CE.app/Contents/MacOS:$PATH"

source /opt/homebrew/share/powerlevel10k/powerlevel10k.zsh-theme

# To customize prompt, run `p10k configure` or edit ~/.p10k.zsh.
[[ ! -f ~/.p10k.zsh ]] || source ~/.p10k.zsh

# history setup
HISTFILE=$HOME/.zhistory
SAVEHIST=1000
HISTSIZE=999
setopt share_history
setopt hist_expire_dups_first
setopt hist_ignore_dups
setopt hist_verify

# completion using arrow keys (based on history)
bindkey '^[[A' history-search-backward
bindkey '^[[B' history-search-forward

source /opt/homebrew/share/zsh-autosuggestions/zsh-autosuggestions.zsh
source /opt/homebrew/share/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
#alias gw='(function _gw() { local dir=$(pwd); while [ ! -x "$dir/gradlew" ] && [ "$dir" != "/" ]; do dir=$(dirname "$dir"); done; if [ -x "$dir/gradlew" ]; then "$dir/gradlew" "$@"; else echo "No gradlew script found"; fi; }; _gw'
alias gw='(cd $(git rev-parse --show-toplevel) && ./gradlew'

#THIS MUST BE AT THE END OF THE FILE FOR SDKMAN TO WORK!!!
export SDKMAN_DIR="$HOME/.sdkman"
[[ -s "$HOME/.sdkman/bin/sdkman-init.sh" ]] && source "$HOME/.sdkman/bin/sdkman-init.sh"

#
# Java Development
# - Quarkus Installation: sdk install quarkus
alias q=quarkus
#source <(quarkus completion)

. "$HOME/.atuin/bin/env"

eval "$(atuin init zsh)"

alias -s md="bat"
alias dotfiles='git --git-dir=$HOME/.dotfiles --work-tree=$HOME'
